//disabled={isLoading}
//onClick={handleDelete}
//type="button"
//>
//{isLoading ? (
//<LoaderWrap>
//<Loader />
//</LoaderWrap>
//) : (
//t('deleteEntryModal.buttonDelete')
//)}
//</PopupBtnRemove>
//</PopupFooter>
//</Popup>
